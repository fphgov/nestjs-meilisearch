{
  "name": "nestjs-meilisearch",
  "version": "2.1.1",
  "description": "Meilisearch integration for nestjs.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rimraf lib && tsc -p tsconfig.build.json",
    "format": "prettier --write '**/*.md' '**/*.json' '**/*.ts' '**/*.yml' .prettierrc",
    "test": "jest",
    "test:cov": "jest --coverage",
    "codecov": "codecov"
  },
  "files": [
    "lib"
  ],
  "repository": "https://github.com/lambrohan/nestjs-meilisearch",
  "keywords": [
    "nestjs",
    "meilisearch"
  ],
  "author": {
    "name": "Rohan Lamb",
    "url": "https://github.com/lambrohan"
  },
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0",
    "meilisearch": ">=0.27.0 <1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.8",
    "@nestjs/core": "^9.0.8",
    "@nestjs/testing": "^9.0.8",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/yargs-parser": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "codecov": "^3.8.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "meilisearch": "^0.27.0",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  },
  "dependencies": {}
}
